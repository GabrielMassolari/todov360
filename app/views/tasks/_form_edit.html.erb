<%= simple_form_for task do |f| %>
  <%= f.input :name %>
  <%= f.input :description, input_html: { rows: 3 } %>
  <%= f.input :priority, collection: [['High', 'high'], ['Medium', 'medium'], ['Low', 'low']] %>
  <%= f.input :status, collection: [['ToDo', 'todo'], ['Progress', 'progress'], ['Done', 'done']] %>
  <%= f.association :list, collection: current_user.lists.all%>
  <%= f.button :submit %>
<% end %>
